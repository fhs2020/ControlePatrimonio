@model ControlePatrimonio.Models.Categoria

@{
    ViewBag.Title = "Details";
}

<h2>Detalhes</h2>

<div>
    <h4>Categoria</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Tipo Categoria
        </dt>

        <dd>
            @Model.TipoCategoria.ToUpper()
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Editar", "Edit", new { id = Model.ID }) |
    @Html.ActionLink("Voltar para lista de categorias", "Index")
</p>
